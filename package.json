{
  "name": "textlint-rule-no-date-without-actual-date",
  "version": "1.0.3",
  "description": "textlint rule that disallow date without actual date.",
  "keywords": [
    "textlintrule"
  ],
  "license": "MIT",
  "author": "Omochice",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "scripts": {
    "fmt": "npm-run-all fmt:*",
    "fmt:package": "sort-package-json",
    "fmt:code": "biome format --write src",
    "lint": "biome check --formatter-enabled true src",
    "test": "textlint-scripts test",
    "build": "textlint-scripts build",
    "prepublish": "npm run --if-present build",
    "watch": "textlint-scripts build --watch"
  },
  "dependencies": {
    "date-fns": "^4.0.0",
    "unknownutil": "^3.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.0.0",
    "@textlint/types": "^14.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^22.0.0",
    "babel-plugin-static-fs": "^3.0.0",
    "npm-run-all2": "^7.0.0",
    "sort-package-json": "^3.0.0",
    "textlint": "^14.0.0",
    "textlint-scripts": "^14.0.0",
    "textlint-tester": "^14.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@10.8.1"
}
