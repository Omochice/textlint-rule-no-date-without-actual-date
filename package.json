{
  "name": "textlint-rule-no-date-without-actual-date",
  "version": "1.0.4",
  "description": "textlint rule that disallow date without actual date.",
  "keywords": [
    "textlintrule"
  ],
  "license": "MIT",
  "author": "Omochice",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "scripts": {
    "build": "textlint-scripts build",
    "fmt": "npm-run-all fmt:*",
    "fmt:code": "biome format --write src",
    "fmt:package": "sort-package-json",
    "lint": "biome check --formatter-enabled true src",
    "prepublish": "npm run --if-present build",
    "test": "textlint-scripts test",
    "watch": "textlint-scripts build --watch"
  },
  "dependencies": {
    "date-fns": "^4.0.0",
    "unknownutil": "^3.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@textlint/types": "15.2.0",
    "@types/mocha": "10.0.10",
    "@types/node": "22.16.5",
    "babel-plugin-static-fs": "3.0.0",
    "npm-run-all2": "8.0.4",
    "sort-package-json": "3.4.0",
    "textlint": "15.2.0",
    "textlint-scripts": "15.2.0",
    "textlint-tester": "14.8.4",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.13.1"
}
