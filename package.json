{
  "name": "textlint-rule-no-date-without-actual-date",
  "version": "1.0.4",
  "description": "textlint rule that disallow date without actual date.",
  "keywords": [
    "textlintrule"
  ],
  "license": "MIT",
  "author": "Omochice",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "scripts": {
    "build": "textlint-scripts build",
    "check": "biome ci",
    "clean-pkg-json": "clean-pkg-json",
    "fmt": "npm-run-all2 fmt:*",
    "fmt:biome:check": "biome check --write --unsafe .",
    "fmt:biome:format": "biome format --write .",
    "fmt:package": "sort-package-json",
    "prepack": "npm-run-all2 --sequential build clean-pkg-json",
    "test": "textlint-scripts test",
    "watch": "textlint-scripts build --watch"
  },
  "dependencies": {
    "date-fns": "^4.0.0",
    "unknownutil": "^3.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@textlint/types": "15.2.2",
    "@types/mocha": "10.0.10",
    "@types/node": "22.18.8",
    "babel-plugin-static-fs": "3.0.0",
    "clean-pkg-json": "1.3.0",
    "npm-run-all2": "8.0.4",
    "sort-package-json": "3.4.0",
    "textlint": "15.2.2",
    "textlint-scripts": "15.2.2",
    "textlint-tester": "15.2.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.18.0"
}
